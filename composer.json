{
    "name": "mikamatto/scheduler-bundle",
    "description": "A Symfony bundle to manage and execute scheduled tasks via cron.",
    "type": "symfony-bundle",
    "keywords": ["symfony", "bundle", "scheduler", "cron"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mikamatto",
            "email": "mick@mikamatto.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/framework-bundle": "^6.0 || ^7.0",
        "doctrine/orm": "^3.0",
        "symfony/cache": "^6.0 || ^7.0",
        "symfony/console": "^6.0 || ^7.0",
        "symfony/form": "^6.0 || ^7.0",
        "symfony/security-bundle": "^6.0 || ^7.0",
        "symfony/validator": "^6.0 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "Mikamatto\\Scheduler\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}